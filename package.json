{
  "name": "naboris",
  "version": "0.0.1",
  "description": "Express-like HTTP server for ocaml/reasonml.  Based on httpaf and lwt.",
  "esy": {
    "build": [
      ["dune"]
    ],
    "install": [
      "esy-installer"
    ],
    "buildInSource": true
  },
  "scripts": {
    "test": "esy b dune exec ./test/fullServerTest.exe",
    "install": "esy",
    "build-main": "esy b dune build @install",
    "build-test": "esy b dune build ./test/fullServerTest.exe @install",
    "build": "npm run build-main && npm run build-test",
    "clean": "esy b dune clean",
    "clean-install": "rm -rf node_modules && rm -rf esy.lock && rm -rf _esy",
    "clean-world": "npm run clean && npm run clean-install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawn-mcginty/naboris.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shawn-mcginty/naboris/issues"
  },
  "homepage": "https://github.com/shawn-mcginty/naboris#readme",
  "dependencies": {
    "@opam/ocamlfind": "*",
    "@opam/dune": "*",
    "@opam/reason": "*",
    "@opam/httpaf": "*",
    "@opam/httpaf-lwt-unix": "*",
    "@opam/lwt": "*",
    "@opam/uri": "*"
  },
  "devDependencies": {
    "ocaml": "*",
    "esy": "^0.5.6",
    "@opam/cohttp": "*",
    "@opam/cohttp-lwt-unix": "*"
  },
  "peerDependencies": {
    "ocaml": "~4.6.1"
  }
}